{
  "name": "jbelien/phpstan-sarif-formatter",
  "description": "SARIF formatter for PHPStan",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "nette/utils": "^3.0 || ^4.0",
    "phpstan/phpstan": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "PHPStanSarifErrorFormatter\\": "src/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}